{
  "name": "@nappr/map",
  "version": "0.2.19",
  "description": "Leaflet Interactive React Map",
  "author": "Matthieu <sixertoy.github@gmail.com>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "map",
    "leaflet",
    "react"
  ],
  "homepage": "https://github.com/sixertoy/nappr-map#readme",
  "type": "module",
  "types": "./types/index.d.ts",
  "main": "./dist/nappr-map.umd.js",
  "module": "./dist/nappr-map.es.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/nappr-map.es.js",
      "require": "./dist/nappr-map.umd.js"
    },
    "./dist/styles.css": "./dist/styles.css"
  },
  "files": [
    "dist/*",
    "types/**/*"
  ],
  "bugs": {
    "url": "https://github.com/sixertoy/nappr-map/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sixertoy/nappr-map.git"
  },
  "dependencies": {
    "leaflet": "^1.9.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-leaflet": "^5.0.0"
  },
  "peerDependencies": {
    "leaflet": "^1.9.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-leaflet": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.3",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "sass": "^1.90.0",
    "stylelint": "^16.23.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-order": "^7.0.0",
    "typescript": "^5.0.4",
    "vite": "^7.0.6",
    "vite-plugin-dts": "^4.5.4"
  },
  "scripts": {
    "cleanup": "rm -rf node_modules && rm -rf dist && rm -rf types",
    "prettier": "prettier . --check",
    "format": "prettier . --write",
    "types": "tsc -p ./tsconfig.json --noEmit",
    "lint": "eslint src/**/*.{ts,tsx} --config=eslint.config.js",
    "prebuild": "rm -rf dist && rm -rf types && yarn types && yarn prettier && yarn lint",
    "build": "tsc -p ./tsconfig.json && vite build"
  }
}
